{
  "name": "mblabs-api",
  "version": "1.0.0",
  "description": "## Introdução",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "ttsc --project tsconfig.json",
    "test": "jest --detectOpenHandles --verbose",
    "test:db-push": "prisma db push && npm test",
    "test:docker": "npm run test:compose -- up",
    "test:compose": "docker-compose -f docker-compose.test.yml",
    "ci:build": "docker-compose -f docker-compose.test.yml build",
    "ci:test": "docker-compose -f docker-compose.test.yml run mblabs-test 'npm run test:db-push'",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/src/server.js",
    "dev": "nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register src/server.ts' -e ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@lab.coodesh.com:v-aguiar/nodejs-20201030.git"
  },
  "keywords": [
    "Node",
    "Typescript",
    "Rest",
    "Mongo",
    "PrismaORM"
  ],
  "author": "Victor Aguiar Ribeiro",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.6",
    "@types/supertest": "^2.0.12",
    "jest": "^29.2.2",
    "nodemon": "^2.0.20",
    "prisma": "^4.3.1",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.4",
    "typescript-transform-paths": "^3.4.4"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "http-status": "^1.5.3",
    "joi": "^17.6.4"
  }
}